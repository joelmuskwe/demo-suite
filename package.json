{
  "name": "demo-suite",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "npm run postinstall.web && ng serve admin-desktop",
    "build": "npm run postinstall.web && ng build admin-desktop",
    "test": "npm run postinstall.web && ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.desktop-app": "npm run prepare.electron.desktop-app && ng build electron-desktop-app --prod --base-href ./",
    "build.electron.desktop-app.local": "npm run build.electron.desktop-app && electron dist/apps/electron-desktop-app",
    "build.electron.desktop-app.linux": "npm run build.electron.desktop-app && cd dist/apps/electron-desktop-app && npx electron-builder build --linux",
    "build.electron.desktop-app.windows": "npm run build.electron.desktop-app && cd dist/apps/electron-desktop-app && npx electron-builder build --windows",
    "build.electron.desktop-app.mac": "npm run build.electron.desktop-app && cd dist/apps/electron-desktop-app && npx electron-builder build --mac",
    "prepare.electron.desktop-app": "npm run postinstall.electron && tsc -p apps/electron-desktop-app/tsconfig.json",
    "serve.electron.desktop-app.target": "nx serve electron-desktop-app",
    "serve.electron.desktop-app": "wait-on http-get://localhost:4200/ && electron apps/electron-desktop-app/src --serve",
    "start.electron.desktop-app": "npm run prepare.electron.desktop-app && npm-run-all -p serve.electron.desktop-app.target serve.electron.desktop-app",
    "start.admin.desktop": "npm run postinstall.web && nx serve admin-desktop"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@nrwl/angular": "^8.8.2",
    "@nstudio/electron": "^8.0.13",
    "@nstudio/web-angular": "^8.0.13",
    "@nstudio/xplat": "^8.0.13",
    "core-js": "^2.5.4",
    "rxjs": "~6.4.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.14",
    "@angular/cli": "8.3.14",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@nrwl/cypress": "8.8.2",
    "@nrwl/jest": "8.8.2",
    "@nrwl/workspace": "8.8.2",
    "@nstudio/xplat": "8.0.13",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "3.4.1",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "prettier": "1.18.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "electron": "^9.4.0",
    "electron-builder": "^20.38.4",
    "electron-rebuild": "~1.8.4",
    "electron-installer-dmg": "~3.0.0",
    "electron-packager": "~13.1.0",
    "electron-reload": "~1.4.0",
    "electron-store": "~2.0.0",
    "electron-updater": "~4.0.6",
    "npm-run-all": "^4.1.5",
    "wait-on": "~3.2.0",
    "@nstudio/electron-angular": "8.0.13",
    "@nrwl/angular": "~8.8.0",
    "@nstudio/electron": "8.0.13",
    "@nstudio/angular": "8.0.13"
  },
  "xplat": {
    "prefix": "demo-suite",
    "framework": "angular"
  }
}
